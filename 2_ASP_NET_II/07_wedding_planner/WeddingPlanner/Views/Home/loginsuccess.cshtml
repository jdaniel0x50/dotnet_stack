@model WeddingPlanner.Models.LoginRegFormModel
@{
    ViewData["Title"] = "Logged In";
}

<div id="fullscreen_bg" class="fullscreen_bg"></div>
<div class="container">
    <div class="row mt-3">
        <div class="col-md-10 mx-auto">
            <div class="panel panel-login p-3">
                <div class="panel-heading">
                    <div class="row p-3">
                        <div class="row col-md-12">
                            <h1>Welcome Back, @Context.Session.GetString("LoggedIn_FirstName")!</h1>
                        </div>
                        <div class="row col-md-12">
                            <h5>You are logged in.</h4>
                        </div>
                    </div>
                </div>
                <hr class="mt-2 mb-3">
            </div>
        </div>
    </div>
</div>
